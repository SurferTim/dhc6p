<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>alt-selt.ac</path>

    <animation>
        <type>material</type>
        <object-name>alt.dwn</object-name>
        <object-name>alt.fond</object-name>
        <object-name>alt.up</object-name>
        <object-name>cadre-altsel</object-name>
        <object-name>vs.dwn</object-name>
        <object-name>vs.fond</object-name>
        <object-name>vs.up</object-name>
        <object-name>vsSet.cancel</object-name>
        <emission>
            <red>0.381924</red>
            <green>0.30935844</green>
            <blue>0.161171928</blue>
            <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>alt.1</object-name>
        <object-name>alt.10</object-name>
        <object-name>alt.100</object-name>
        <object-name>alt.1000</object-name>
        <object-name>alt.10000</object-name>
        <object-name>vs.1</object-name>
        <object-name>vs.10</object-name>
        <object-name>vs.100</object-name>
        <object-name>vs.1000</object-name>
        <object-name>vs.minus</object-name>
        <object-name>alt.knob</object-name>
        <object-name>vs.knob</object-name>
        <condition>
            <less-than>
                <property>controls/lighting/plt-eng-inst-emer-pnl-lts</property>
                <value>0.05</value>
            </less-than>
        </condition>
        <emission>
            <red>0.381924</red>
            <green>0.30935844</green>
            <blue>0.161171928</blue>
            <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

