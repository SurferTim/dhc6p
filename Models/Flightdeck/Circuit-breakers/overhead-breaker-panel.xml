<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>overhead-breaker-panel.ac</path>

    <animation>
        <type>material</type>
        <object-name>throttleQ</object-name>
        <object-name>fuse-panel</object-name>
        <object-name>ohpanel-light.base</object-name>
        <object-name>ohpanel-light.switch</object-name>
        <object-name>ohpanel-light</object-name>
        <emission>
            <red>0.381924</red>
            <green>0.30935844</green>
            <blue>0.161171928</blue>
            <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>labels</object-name>
        <object-name>gen-control-l</object-name>
        <object-name>gen-control-r</object-name>
        <object-name>inverter-1</object-name>
        <object-name>inverter-2</object-name>
        <object-name>unused</object-name>
        <object-name>ws-heat-l</object-name>
        <object-name>ws-heat-r</object-name>
        <object-name>flt-compt-fans</object-name>
        <object-name>doors-unlocked</object-name>
        <object-name>reading</object-name>
        <object-name>cabin-lts</object-name>
        <object-name>general</object-name>
        <object-name>emer-lts</object-name>
        <object-name>cabin-vent-fan</object-name>
        <object-name>pneumatic-press</object-name>
        <object-name>cockpit-fans</object-name>
        <condition>
            <equals>
                <property>controls/lighting/instrument-lights</property>
                <value>0</value>
            </equals>
        </condition>
        <emission>
              <red>0.381924</red>
              <green>0.30935844</green>
              <blue>0.161171928</blue>
              <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>ohpanel-light.lit</object-name>
        <condition>
            <equals>
                <property>controls/lighting/overhead-light</property>
                <value>0</value>
            </equals>
        </condition>
        <emission>
              <red>0.381924</red>
              <green>0.30935844</green>
              <blue>0.161171928</blue>
              <factor-prop>sim/multiplay/generic/float[10]</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>ohpanel-light.lit</object-name>
        <condition>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
             <red>1</red>
             <green>0.9</green>
             <blue>0.8</blue>
             <factor-prop>systems/electrical/outputs/overhead-light-dim-factor</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
